{
  "compilerOptions": {
	"sourceMap": true,
	"rootDir": "src",
	"allowSyntheticDefaultImports": true,
	"strict": true,
	"forceConsistentCasingInFileNames": true,
	"moduleResolution": "node",
	"skipLibCheck": true,
	"importHelpers": true,
	"isolatedModules": true,
	"declaration": true,
	"declarationMap": true,
	"esModuleInterop": true
  }
}
