{
	"name": "zod-validate-env",
  	"sideEffects": false,
	"version": "0.0.0",
	"description": "",
	"main": "dist/cjs/index.js",
	"private": false,
	"scripts": {
	  	"test": "echo \"will add tests soon\"",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "pnpm build && changeset publish",
		"format": "prettier --write './src/**/*.{js,ts}'",
		"lint": "eslint './src/**/*.{js,ts}' --fix",
		"start": "node dist/cjs/index.js",
		"dev": "NODE_ENV=development nodemon dist/cjs/index.js",
		"start:dev": "concurrently -k \"pnpm watch\" \"pnpm dev\"",
		"clean": "rimraf dist",
		"build:cjs": "tsc --project tsconfig.cjs.json",
		"build:esm": "tsc --project tsconfig.json",
		"build": "pnpm clean && pnpm build:cjs && pnpm build:esm",
		"watch": "tsc -w --project tsconfig.cjs.json",
		"prepare": "husky install",
		"commit": "git add . && git-cz",
		"typecheck": "tsc --noEmit --project tsconfig.cjs.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@changesets/changelog-git": "^0.1.12",
		"@changesets/cli": "^2.22.0",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@types/node": "^18.7.14",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"commitizen": "^4.2.5",
		"concurrently": "^7.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"editorconfig": "^0.15.3",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^22.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^8.0.1",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"zod": "^3.18.0"
	}
}
